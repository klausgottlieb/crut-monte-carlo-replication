    "# =============================================================================\n",
    "# RETURN MODEL\n",
    "# Generates Monte Carlo return paths under Gaussian (GBM) assumptions.\n",
    "# =============================================================================\n",
    "\n",
    "def generate_return_paths(\n",
    "    mu: float,\n",
    "    sigma: float,\n",
    "    n_years: int,\n",
    "    n_paths: int,\n",
    "    seed: Optional[int] = None,\n",
    ") -> np.ndarray:\n",
    "    \"\"\"Generate lognormal annual return paths (Gaussian / GBM model).\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    mu : float\n",
    "        Expected arithmetic annual return (e.g., 0.07 for 7%).\n",
    "    sigma : float\n",
    "        Annual return volatility (standard deviation, e.g., 0.12 for 12%).\n",
    "    n_years : int\n",
    "        Number of years per simulation path.\n",
    "    n_paths : int\n",
    "        Number of independent Monte Carlo paths.\n",
    "    seed : int or None\n",
    "        Random seed for reproducibility.\n",
    "        \n",
    "    Returns\n",
    "    -------\n",
    "    np.ndarray of shape (n_paths, n_years)\n",
    "        Gross annual return factors (e.g., 1.07 for a 7% return year).\n",
    "    \"\"\"\n",
    "    if seed is not None:\n",
    "        np.random.seed(seed)\n",
    "    \n",
    "    # Convert arithmetic moments to log-space parameters\n",
    "    # This preserves E[r] = mu and Std[r] = sigma\n",
    "    mu_log = np.log(1 + mu) - 0.5 * (sigma / (1 + mu)) ** 2\n",
    "    sigma_log = sigma / (1 + mu)\n",
    "    \n",
    "    # Draw log-returns and exponentiate to get gross return factors\n",
    "    log_returns = np.random.normal(mu_log, sigma_log, size=(n_paths, n_years))\n",
    "    return np.exp(log_returns)\n",
    "\n",
    "\n",
    "# Quick verification: mean and std of simulated returns should match inputs\n",
    "test_returns = generate_return_paths(0.07, 0.12, 30, 10_000, seed=0)\n",
    "gross = test_returns.flatten()\n",
    "print(f\"Return model verification (10,000 paths × 30 years):\")\n",
    "print(f\"  Target mean:  7.00%  |  Simulated: {(gross.mean()-1)*100:.2f}%\")\n",
    "print(f\"  Target sigma: 12.00% |  Simulated: {gross.std()*100:.2f}%\")\n",
    "print(f\"  Min annual return: {(gross.min()-1)*100:.1f}%\")\n",
    "print(f\"  Max annual return: {(gross.max()-1)*100:.1f}%\")"
   ],
