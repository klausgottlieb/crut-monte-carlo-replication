    "# =============================================================================\n",
    "# SIMULATION ENGINE\n",
    "# Simulates CRUT and benchmark portfolios over paired Monte Carlo paths.\n",
    "# =============================================================================\n",
    "\n",
    "@dataclass\n",
    "class ScenarioParams:\n",
    "    \"\"\"Complete parameter set for a CRUT vs. benchmark simulation.\n",
    "    \n",
    "    All rates are expressed as decimals (e.g., 0.06 for 6%).\n",
    "    Dollar amounts are in nominal terms.\n",
    "    \"\"\"\n",
    "    # --- Contributed asset ---\n",
    "    fmv: float = 1_000_000       # Fair market value\n",
    "    basis_pct: float = 0.20      # Basis as fraction of FMV (0 = fully appreciated)\n",
    "    agi: float = 500_000         # Donor adjusted gross income\n",
    "    \n",
    "    # --- CRUT design ---\n",
    "    payout_rate: float = 0.06    # Stated annual payout rate\n",
    "    life_type: str = 'Two Life'  # 'Single Life', 'Two Life', 'Term of Years'\n",
    "    age1: int = 63               # Primary beneficiary age\n",
    "    age2: Optional[int] = 65     # Second beneficiary age (Two Life only)\n",
    "    term_years: int = 20         # Term (Term of Years only)\n",
    "    freq: int = 4                # Payment frequency (4 = quarterly)\n",
    "    lag_months: int = 0          # Months lag to first payment\n",
    "    longevity_adj: int = 0       # Years beyond IRS horizon to simulate\n",
    "    \n",
    "    # --- Actuarial / valuation ---\n",
    "    rate_7520: float = 0.05      # §7520 rate\n",
    "    pv_rate: float = 0.05        # Present-value discount rate\n",
    "    \n",
    "    # --- Tax rates ---\n",
    "    fed_ordinary: float = 0.37   # Federal ordinary income rate\n",
    "    fed_ltcg: float = 0.20       # Federal long-term capital gains rate\n",
    "    niit: float = 0.038          # Net investment income tax (§1411)\n",
    "    state_rate: float = 0.093    # State income tax rate (9.3% = California)\n",
    "    \n",
    "    # --- AGI limits (IRC §170) ---\n",
    "    agi_limit_pct: float = 0.30  # Appreciated property to public charity\n",
    "    carryforward_years: int = 5  # Years of deduction carryforward\n",
    "    \n",
    "    # --- Fees ---\n",
    "    trust_fee: float = 0.01      # Annual trustee fee\n",
    "    bench_fee: float = 0.01      # Annual benchmark management fee\n",
    "    turnover: float = 0.60       # Benchmark annual portfolio turnover\n",
    "    \n",
    "    # --- Return model ---\n",
    "    mu: float = 0.07             # Expected arithmetic annual return\n",
    "    sigma: float = 0.12          # Annual return volatility\n",
    "    n_paths: int = 2000          # Monte Carlo paths\n",
    "    seed: int = 42               # Random seed\n",
    "\n",
    "\n",
    "def run_simulation(params: ScenarioParams) -> dict:\n",
    "    \"\"\"Run the full CRUT vs. benchmark Monte Carlo simulation.\n",
    "    \n",
    "    Returns a dict containing:\n",
    "        deduction_res   : dict — actuarial deduction results\n",
    "        crut_wealth     : ndarray (n_paths,) — CRUT terminal personal wealth\n",
    "        bench_wealth    : ndarray (n_paths,) — benchmark terminal wealth\n",
    "        delta_wealth    : ndarray (n_paths,) — difference (CRUT - benchmark)\n",
    "        win_prob        : float — fraction of paths where CRUT wins\n",
    "        median_delta    : float — median terminal net benefit\n",
    "        crut_paths      : ndarray (n_paths, T+1) — CRUT corpus over time\n",
    "        bench_paths     : ndarray (n_paths, T+1) — benchmark value over time\n",
    "        distributions   : ndarray (n_paths, T) — annual after-tax distributions\n",
    "        params          : ScenarioParams — input parameters (for reference)\n",
    "    \"\"\"\n",
    "    p = params\n",
    "    \n",
    "    # -------------------------------------------------------------------------\n",
    "    # Step 1: Actuarial deduction\n",
    "    # -------------------------------------------------------------------------\n",
    "    ded_res = compute_deduction(\n",
    "        fmv=p.fmv, payout_rate=p.payout_rate, rate_7520=p.rate_7520,\n",
    "        life_type=p.life_type, age1=p.age1, age2=p.age2,\n",
    "        term_years=p.term_years, freq=p.freq, lag_months=p.lag_months,\n",
    "        longevity_adj=p.longevity_adj,\n",
    "    )\n",
    "    T = int(round(ded_res['sim_horizon']))\n",
    "    deduction = ded_res['deduction']\n",
    "    \n",
    "    # -------------------------------------------------------------------------\n",
    "    # Step 2: Compute present value of tax benefit from charitable deduction\n",
    "    # Deduction is spread over current year + carryforward years,\n",
    "    # subject to the AGI limit each year.\n",
    "    # -------------------------------------------------------------------------\n",
    "    combined_ordinary = p.fed_ordinary + p.state_rate  # combined marginal rate\n",
    "    annual_agi_limit = p.agi * p.agi_limit_pct\n",
    "    \n",
    "    remaining_deduction = deduction\n",
    "    pv_tax_benefit = 0.0\n",
    "    for yr in range(p.carryforward_years + 1):\n",
    "        usable = min(remaining_deduction, annual_agi_limit)\n",
    "        if usable <= 0:\n",
    "            break\n",
    "        tax_saving = usable * combined_ordinary\n",
    "        pv_tax_benefit += tax_saving / (1 + p.pv_rate) ** yr\n",
    "        remaining_deduction -= usable\n",
    "    \n",
    "    # -------------------------------------------------------------------------\n",
    "    # Step 3: Generate return paths (paired: same paths for both strategies)\n",
    "    # -------------------------------------------------------------------------\n",
    "    returns = generate_return_paths(p.mu, p.sigma, T, p.n_paths, seed=p.seed)\n",
    "    # returns shape: (n_paths, T)\n",
    "    \n",
    "    # -------------------------------------------------------------------------\n",
    "    # Step 4: Combined tax rates\n",
    "    # -------------------------------------------------------------------------\n",
    "    # CRUT distributions: taxed as ordinary income (federal + state)\n",
    "    tau_ord = p.fed_ordinary + p.state_rate\n",
    "    # Benchmark LTCG: federal LTCG + NIIT + state (CA taxes LTCG as ordinary)\n",
    "    tau_cg = p.fed_ltcg + p.niit + p.state_rate\n",
    "    \n",
    "    # -------------------------------------------------------------------------\n",
    "    # Step 5: Simulate CRUT corpus path\n",
    "    # -------------------------------------------------------------------------\n",
    "    crut_paths = np.zeros((p.n_paths, T + 1))\n",
    "    crut_paths[:, 0] = p.fmv  # Trust funded at FMV of contributed asset\n",
    "    \n",
    "    distributions = np.zeros((p.n_paths, T))  # after-tax distributions\n",
    "    \n",
    "    for t in range(T):\n",
    "        V_prev = crut_paths[:, t]\n",
    "        # Apply fees first (beginning-of-year)\n",
    "        V_after_fee = V_prev * (1 - p.trust_fee)\n",
    "        # Apply return\n",
    "        V_grown = V_after_fee * returns[:, t]\n",
    "        # Distribute payout rate of grown value\n",
    "        dist_gross = V_grown * p.payout_rate\n",
    "        # After-tax distribution (ordinary income taxation)\n",
    "        distributions[:, t] = dist_gross * (1 - tau_ord)\n",
    "        # Trust corpus after distribution\n",
    "        crut_paths[:, t+1] = np.maximum(0, V_grown - dist_gross)\n",
    "    \n",
    "    # -------------------------------------------------------------------------\n",
    "    # Step 6: Simulate benchmark portfolio\n",
    "    # The benchmark starts at FMV; the tax savings from the deduction\n",
    "    # are invested in the taxable account at inception.\n",
    "    # This ensures both strategies begin from equivalent economic positions.\n",
    "    # -------------------------------------------------------------------------\n",
    "    bench_paths = np.zeros((p.n_paths, T + 1))\n",
    "    bench_paths[:, 0] = p.fmv  # Same initial value as CRUT\n",
    "    # NOTE: pv_tax_benefit is added to CRUT wealth at the end, not here,\n",
    "    # to maintain comparability. The benchmark does NOT receive the tax benefit.\n",
    "    \n",
    "    bench_basis = p.fmv * p.basis_pct  # Initial cost basis\n",
    "    \n",
    "    for t in range(T):\n",
    "        B_prev = bench_paths[:, t]\n",
    "        # Apply management fee\n",
    "        B_after_fee = B_prev * (1 - p.bench_fee)\n",
    "        # Apply return\n",
    "        B_grown = B_after_fee * returns[:, t]\n",
    "        # Annual turnover: realize fraction theta of gains, pay LTCG tax\n",
    "        gain = np.maximum(0, B_grown - bench_basis)\n",
    "        tax_drag = p.turnover * gain * tau_cg\n",
    "        bench_paths[:, t+1] = np.maximum(0, B_grown - tax_drag)\n",
    "        # Basis grows with retained (non-distributed) gains\n",
    "        bench_basis = bench_basis + p.turnover * gain * (1 - tau_cg)\n",
    "    \n",
    "    # -------------------------------------------------------------------------\n",
    "    # Step 7: Terminal wealth comparison\n",
    "    # CRUT personal wealth = PV of tax benefit + PV of after-tax distributions\n",
    "    # Benchmark wealth = after-tax liquidation value\n",
    "    # -------------------------------------------------------------------------\n",
    "    \n",
    "    # PV of CRUT after-tax distributions\n",
    "    discount_factors = np.array([(1 + p.pv_rate) ** -(t+1) for t in range(T)])\n",
    "    pv_distributions = (distributions * discount_factors[np.newaxis, :]).sum(axis=1)\n",
    "    \n",
    "    # CRUT personal wealth = PV distributions + PV of tax benefit\n",
    "    crut_wealth = pv_distributions + pv_tax_benefit\n",
    "    \n",
    "    # Benchmark: liquidate at terminal value, pay LTCG on remaining gains\n",
    "    terminal_bench = bench_paths[:, -1]\n",
    "    terminal_gain = np.maximum(0, terminal_bench - bench_basis)\n",
    "    bench_wealth_liquidation = terminal_bench - terminal_gain * tau_cg\n",
    "    \n",
    "    # Discount benchmark to present value\n",
    "    bench_wealth = bench_wealth_liquidation / (1 + p.pv_rate) ** T\n",
    "    \n",
    "    # Terminal net benefit\n",
    "    delta_wealth = crut_wealth - bench_wealth\n",
    "    \n",
    "    # Summary metrics\n",
    "    win_prob = float(np.mean(delta_wealth > 0))\n",
    "    median_delta = float(np.median(delta_wealth))\n",
    "    \n",
    "    return {\n",
    "        'deduction_res': ded_res,\n",
    "        'crut_wealth': crut_wealth,\n",
    "        'bench_wealth': bench_wealth,\n",
    "        'delta_wealth': delta_wealth,\n",
    "        'win_prob': win_prob,\n",
    "        'median_delta': median_delta,\n",
    "        'crut_paths': crut_paths,\n",
    "        'bench_paths': bench_paths,\n",
    "        'distributions': distributions,\n",
    "        'pv_tax_benefit': pv_tax_benefit,\n",
    "        'T': T,\n",
    "        'tau_ord': tau_ord,\n",
    "        'tau_cg': tau_cg,\n",
    "        'params': p,\n",
    "    }\n",
    "\n",
    "print(\"Simulation engine defined. Ready for baseline run.\")"
   ],
   "id": "cell-engine"
  },
