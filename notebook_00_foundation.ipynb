    "# =============================================================================\n",
    "# ACTUARIAL ENGINE\n",
    "# Computes the IRS charitable deduction under IRC §7520 for standard CRUTs.\n",
    "# Supports: Single Life, Two Life, Term of Years.\n",
    "# Reference: Gottlieb (2025), \"From myth to math: A tutorial on the actuarial\n",
    "#            valuation of charitable remainder unitrusts under I.R.C. § 7520.\"\n",
    "# =============================================================================\n",
    "\n",
    "# IRS life expectancy table (Table 2000CM, single life, selected ages)\n",
    "# Source: IRS Publication 1457 (2009 revision, effective through current use)\n",
    "# Key: age (integer) -> life expectancy in years\n",
    "IRS_LIFE_EXPECTANCY = {\n",
    "    35: 48.3, 40: 43.6, 45: 38.9, 50: 34.2, 55: 29.6,\n",
    "    60: 25.2, 61: 24.4, 62: 23.5, 63: 22.7, 64: 21.8,\n",
    "    65: 21.0, 66: 20.2, 67: 19.4, 68: 18.6, 69: 17.8,\n",
    "    70: 17.0, 71: 16.3, 72: 15.5, 73: 14.8, 74: 14.1,\n",
    "    75: 13.4, 76: 12.7, 77: 12.1, 78: 11.4, 79: 10.8,\n",
    "    80: 10.2, 85: 7.3, 90: 5.0\n",
    "}\n",
    "\n",
    "def get_life_expectancy(age: int) -> float:\n",
    "    \"\"\"Return IRS single-life expectancy for a given age.\n",
    "    Interpolates linearly for ages not in the table.\"\"\"\n",
    "    if age in IRS_LIFE_EXPECTANCY:\n",
    "        return IRS_LIFE_EXPECTANCY[age]\n",
    "    ages = sorted(IRS_LIFE_EXPECTANCY.keys())\n",
    "    for i in range(len(ages) - 1):\n",
    "        if ages[i] < age < ages[i+1]:\n",
    "            a1, a2 = ages[i], ages[i+1]\n",
    "            e1, e2 = IRS_LIFE_EXPECTANCY[a1], IRS_LIFE_EXPECTANCY[a2]\n",
    "            return e1 + (e2 - e1) * (age - a1) / (a2 - a1)\n",
    "    raise ValueError(f\"Age {age} outside supported range.\")\n",
    "\n",
    "def get_joint_life_expectancy(age1: int, age2: int) -> float:\n",
    "    \"\"\"Approximate two-life (last-to-die) expectancy.\n",
    "    \n",
    "    The IRS uses Table 2000CM with a specific joint-life computation.\n",
    "    This implementation uses the standard approximation:\n",
    "        E[max(T1, T2)] = E[T1] + E[T2] - E[min(T1, T2)]\n",
    "    where E[min] is approximated via independent exponential assumption.\n",
    "    For planning purposes this is sufficiently accurate; precise values\n",
    "    require IRS actuarial workbooks.\n",
    "    \"\"\"\n",
    "    e1 = get_life_expectancy(age1)\n",
    "    e2 = get_life_expectancy(age2)\n",
    "    # Joint last-to-die approximation\n",
    "    e_joint_first = 1.0 / (1.0/e1 + 1.0/e2)\n",
    "    return e1 + e2 - e_joint_first\n",
    "\n",
    "def table_f_factor(rate_7520: float, freq: int, lag_months: int = 0) -> float:\n",
    "    \"\"\"Compute the IRS Table F timing adjustment factor.\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    rate_7520 : float\n",
    "        The applicable federal rate under §7520 (e.g., 0.05 for 5%).\n",
    "    freq : int\n",
    "        Payment frequency per year (1=annual, 2=semi, 4=quarterly, 12=monthly).\n",
    "    lag_months : int\n",
    "        Months between trust valuation date and first payment (0, 1, or 2\n",
    "        for quarterly; 0 only for monthly).\n",
    "        \n",
    "    Returns\n",
    "    -------\n",
    "    float\n",
    "        The Table F adjustment factor.\n",
    "    \"\"\"\n",
    "    if rate_7520 <= 0:\n",
    "        raise ValueError(\"§7520 rate must be positive.\")\n",
    "    max_lag = {1: 0, 2: 0, 4: 2, 12: 0}.get(freq, 0)\n",
    "    if lag_months > max_lag:\n",
    "        raise ValueError(f\"Lag {lag_months} months exceeds IRS maximum {max_lag} for freq={freq}.\")\n",
    "    i = rate_7520\n",
    "    k = freq\n",
    "    # Period rate\n",
    "    period_rate = (1 + i) ** (1/k) - 1\n",
    "    # Table F factor (end-of-period timing baseline)\n",
    "    f = period_rate / (i / k)\n",
    "    # Lag adjustment: advance by lag_months/12 of a year\n",
    "    if lag_months > 0:\n",
    "        f *= (1 + i) ** (lag_months / 12)\n",
    "    return f\n",
    "\n",
    "def compute_deduction(\n",
    "    fmv: float,\n",
    "    payout_rate: float,\n",
    "    rate_7520: float,\n",
    "    life_type: str,      # 'Single Life', 'Two Life', 'Term of Years'\n",
    "    age1: int = 65,\n",
    "    age2: Optional[int] = None,\n",
    "    term_years: int = 20,\n",
    "    freq: int = 4,\n",
    "    lag_months: int = 0,\n",
    "    longevity_adj: int = 0,\n",
    ") -> dict:\n",
    "    \"\"\"Compute the IRS charitable deduction for a standard CRUT.\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    fmv : float\n",
    "        Fair market value of contributed asset.\n",
    "    payout_rate : float\n",
    "        Stated annual payout rate (e.g., 0.06 for 6%).\n",
    "    rate_7520 : float\n",
    "        Applicable §7520 rate.\n",
    "    life_type : str\n",
    "        Trust duration basis.\n",
    "    age1 : int\n",
    "        Age of primary beneficiary (for life trusts).\n",
    "    age2 : int or None\n",
    "        Age of second beneficiary (for two-life trusts; None otherwise).\n",
    "    term_years : int\n",
    "        Fixed term (for Term of Years trusts).\n",
    "    freq : int\n",
    "        Payment frequency per year.\n",
    "    lag_months : int\n",
    "        Months lag to first payment.\n",
    "    longevity_adj : int\n",
    "        Years added to IRS actuarial horizon for simulation planning\n",
    "        (does NOT affect the deduction calculation, only sim horizon).\n",
    "        \n",
    "    Returns\n",
    "    -------\n",
    "    dict with keys:\n",
    "        deduction : float — charitable deduction amount\n",
    "        remainder_factor : float — R (must be >= 0.10 for compliance)\n",
    "        compliance : bool — True if 10% remainder test is satisfied\n",
    "        irs_duration : float — simulation horizon in years\n",
    "        msg : str — human-readable summary\n",
    "    \"\"\"\n",
    "    p = payout_rate\n",
    "    i = rate_7520\n",
    "    \n",
    "    # Step 1: Table F timing factor\n",
    "    f = table_f_factor(i, freq, lag_months)\n",
    "    \n",
    "    # Step 2: Adjusted annual payout rate\n",
    "    p_adj = 1.0 - (1.0 - p) * f\n",
    "    # Clamp to valid range to prevent floating-point issues\n",
    "    p_adj = max(0.001, min(0.999, p_adj))\n",
    "    \n",
    "    # Step 3: IRS actuarial horizon\n",
    "    if life_type == 'Term of Years':\n",
    "        irs_duration = float(term_years)\n",
    "    elif life_type == 'Single Life':\n",
    "        irs_duration = get_life_expectancy(age1)\n",
    "    elif life_type == 'Two Life':\n",
    "        if age2 is None:\n",
    "            raise ValueError(\"age2 required for Two Life trust.\")\n",
    "        irs_duration = get_joint_life_expectancy(age1, age2)\n",
    "    else:\n",
    "        raise ValueError(f\"Unknown life_type: {life_type}\")\n",
    "    \n",
    "    # Step 4: Remainder factor\n",
    "    R = p_adj ** irs_duration\n",
    "    R = max(0.0, min(1.0, R))\n",
    "    \n",
    "    # Step 5: Deduction and compliance check\n",
    "    deduction = fmv * R\n",
    "    compliance = R >= 0.10\n",
    "    \n",
    "    # Simulation horizon includes any longevity adjustment\n",
    "    sim_horizon = irs_duration + longevity_adj\n",
    "    \n",
    "    msg = (\n",
    "        f\"{life_type} | p={p:.1%} | i={i:.1%} | \"\n",
    "        f\"IRS horizon={irs_duration:.1f}yr | R={R:.4f} | \"\n",
    "        f\"Deduction=${deduction:,.0f} | \"\n",
    "        f\"{'COMPLIANT' if compliance else 'NON-COMPLIANT'}\"\n",
    "    )\n",
    "    \n",
    "    return {\n",
    "        'deduction': deduction,\n",
    "        'remainder_factor': R,\n",
    "        'compliance': compliance,\n",
    "        'irs_duration': irs_duration,\n",
    "        'sim_horizon': sim_horizon,\n",
    "        'msg': msg,\n",
    "    }"
   ],
   "id": "cell-actuarial"
